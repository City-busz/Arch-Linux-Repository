<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node name="/org/ayatana/indicator/network.agent">
  <interface name="org.ayatana.indicator.network.agent">
    <method name="Start">
      <annotation name="org.freedesktop.DBus.GLib.CSymbol"
		  value="network_agent_start"/>
    </method>
    <method name="Stop">
      <annotation name="org.freedesktop.DBus.GLib.CSymbol"
		  value="network_agent_stop"/>
    </method>
    <method name="ShowConnectError">
      <annotation name="org.freedesktop.DBus.GLib.CSymbol"
		  value="network_agent_show_connect_error"/>
      <arg type="s" name="error" direction="in"/>
    </method>
    <method name="ShowWirelessConnect">
      <annotation name="org.freedesktop.DBus.GLib.CSymbol"
		  value="network_agent_show_wireless_connect"/>
    </method>
    <method name="SetDebug">
      <arg type="i" direction="in"/>
      <annotation name="org.freedesktop.DBus.GLib.CSymbol"
		  value="network_agent_set_debug"/>
    </method>
  </interface>
</node>
