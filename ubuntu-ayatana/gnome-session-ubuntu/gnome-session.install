post_install() {
  usr/bin/glib-compile-schemas usr/share/glib-2.0/schemas
  gtk-update-icon-cache -q -t -f usr/share/icons/hicolor
}

pre_upgrade() {
  if (( $(vercmp $2 2.91.91.3-3) < 0 )); then
    usr/sbin/gconfpkg --uninstall gnome-session
  fi
}

post_upgrade() {
  post_install
}

post_remove() {
  post_install
}
