pkgname=mint-backgrounds-katya-extra
pkgver=1.0.1
pkgrel=1
pkgdesc="Extra desktop backgrounds made for Linux Mint 11 Katya"
arch=('any')
url="http://packages.linuxmint.com/pool/main/m/mint-backgrounds-katya-extra/"
license=('GPL')
source=(http://packages.linuxmint.com/pool/main/m/$pkgname/${pkgname}_$pkgver.tar.gz)
md5sums=('287255beb87c2e0104887abb912e3718')

build() {
  /bin/true
}

package() {
  cd "$srcdir/$pkgname"

  cp -r usr $pkgdir/
}
