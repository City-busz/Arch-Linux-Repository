pkgname=mint-backgrounds-katya
pkgver=1.0.1
pkgrel=1
pkgdesc="The desktop backgrounds made for Linux Mint 11 Katya"
arch=('any')
url="http://packages.linuxmint.com/pool/main/m/mint-backgrounds-katya/"
license=('GPL')
source=(http://packages.linuxmint.com/pool/main/m/$pkgname/${pkgname}_$pkgver.tar.gz)
md5sums=('df6aad963763da79504b62c17171b84d')

build() {
  /bin/true
}

package() {
  cd "$srcdir/$pkgname-${pkgver%.*}"

  cp -r usr $pkgdir/
}
